{
    "manifest_version": 2,
 
    "name": "HR link descriptor",
    "description": "no desc",
    "version": "1.0",
 
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.js"
    },

    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },

    "permissions": [
		"tabs",
        "activeTab",
		"storage"
    ]
}